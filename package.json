{
    "name": "@liaoliaots/nestjs-redis",
    "version": "6.0.0",
    "description": "Redis(ioredis) module for NestJS framework",
    "author": "LiaoLiao",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "health"
    ],
    "license": "MIT",
    "keywords": [
        "redis",
        "ioredis",
        "sentinel",
        "cluster",
        "health",
        "nestjs",
        "nest",
        "nodejs",
        "node",
        "typescript"
    ],
    "scripts": {
        "prepare": "husky install",
        "prebuild": "rimraf dist",
        "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
        "publish:next": "npm publish --tag next",
        "publish:local": "npm publish --registry http://localhost:4873/",
        "lint": "concurrently \"tsc -p tsconfig.json --noEmit\" \"eslint \"{lib,test}/**/*.ts\"\"",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "test:clear": "jest --clearCache",
        "make-badges": "istanbul-badges-readme --style=\"flat-square\"",
        "madge": "madge --image docs/latest/dependency-graph.svg dist/index.js health/index.js"
    },
    "dependencies": {
        "@types/promise.allsettled": "1.0.3",
        "promise.allsettled": "1.0.5",
        "redis-errors": "1.2.0",
        "tslib": "2.3.1"
    },
    "devDependencies": {
        "@commitlint/cli": "16.2.1",
        "@commitlint/config-conventional": "16.2.1",
        "@nestjs/common": "8.3.1",
        "@nestjs/core": "8.3.1",
        "@nestjs/platform-fastify": "8.3.1",
        "@nestjs/terminus": "8.0.4",
        "@nestjs/testing": "8.3.1",
        "@types/ioredis": "4.28.8",
        "@types/jest": "27.4.0",
        "@types/node": "14.18.12",
        "@types/redis-errors": "1.2.1",
        "@typescript-eslint/eslint-plugin": "5.12.0",
        "@typescript-eslint/parser": "5.12.0",
        "concurrently": "7.0.0",
        "eslint": "8.9.0",
        "eslint-config-prettier": "8.4.0",
        "eslint-plugin-deprecation": "1.3.2",
        "eslint-plugin-jest": "26.1.1",
        "eslint-plugin-prettier": "4.0.0",
        "eslint-plugin-promise": "6.0.0",
        "eslint-plugin-tsdoc": "0.2.14",
        "fastify": "3.27.1",
        "husky": "7.0.4",
        "ioredis": "4.28.5",
        "istanbul-badges-readme": "1.8.1",
        "jest": "27.5.1",
        "lint-staged": "12.3.4",
        "prettier": "2.5.1",
        "rimraf": "3.0.2",
        "ts-jest": "27.1.3",
        "tsc-alias": "1.6.1",
        "typescript": "4.5.5"
    },
    "peerDependencies": {
        "@nestjs/common": "8.x || 7.x",
        "@nestjs/core": "8.x || 7.x",
        "@nestjs/terminus": "8.x || 7.x",
        "ioredis": "4.x"
    },
    "peerDependenciesMeta": {
        "@nestjs/terminus": {
            "optional": true
        }
    },
    "engines": {
        "node": ">= 10.13.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "lib",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node",
        "verbose": true,
        "coveragePathIgnorePatterns": [
            "/node_modules/",
            "/interfaces/"
        ],
        "moduleNameMapper": {
            "@/(.*)": [
                "<rootDir>/$1"
            ]
        },
        "coverageReporters": [
            "json-summary"
        ]
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "publishConfig": {
        "access": "public"
    },
    "homepage": "https://github.com/liaoliaots/nestjs-redis#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/liaoliaots/nestjs-redis.git"
    },
    "bugs": {
        "url": "https://github.com/liaoliaots/nestjs-redis/issues"
    }
}
