{
    "name": "@liaoliaots/nestjs-redis",
    "version": "2.1.0",
    "description": "Redis(ioredis) module for NestJS framework",
    "author": "LiaoLiao",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "license": "MIT",
    "keywords": [
        "redis",
        "cluster",
        "ioredis",
        "health",
        "check",
        "nestjs",
        "nest",
        "nodejs",
        "node",
        "javascript",
        "typescript"
    ],
    "scripts": {
        "prepare": "husky install",
        "prebuild": "rimraf dist",
        "build": "tsc -p tsconfig.build.json",
        "lint": "eslint \"{lib,test}/**/*.ts\"",
        "lint:tsc": "tsc -p tsconfig.json --noEmit",
        "publish:next": "npm publish --tag next",
        "test": "jest",
        "test:cov": "jest --coverage",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "test:clear": "jest --clearCache",
        "make-badges": "istanbul-badges-readme",
        "madge": "madge --image ./dependency-graph.svg dist/index.js"
    },
    "dependencies": {
        "tslib": "^2.3.0"
    },
    "devDependencies": {
        "@commitlint/cli": "12.1.4",
        "@commitlint/config-conventional": "12.1.4",
        "@nestjs/axios": "0.0.1",
        "@nestjs/common": "8.0.1",
        "@nestjs/core": "8.0.1",
        "@nestjs/platform-fastify": "8.0.1",
        "@nestjs/terminus": "8.0.0-next.0",
        "@nestjs/testing": "8.0.1",
        "@types/ioredis": "4.26.5",
        "@types/jest": "26.0.24",
        "@types/node": "14.17.5",
        "@typescript-eslint/eslint-plugin": "4.28.2",
        "@typescript-eslint/parser": "4.28.2",
        "eslint": "7.30.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-eslint-comments": "3.2.0",
        "eslint-plugin-jest": "24.3.6",
        "eslint-plugin-prettier": "3.4.0",
        "eslint-plugin-tsdoc": "0.2.14",
        "husky": "7.0.1",
        "ioredis": "4.27.6",
        "istanbul-badges-readme": "1.4.0",
        "jest": "27.0.6",
        "lint-staged": "11.0.0",
        "prettier": "2.3.2",
        "rimraf": "3.0.2",
        "ts-jest": "27.0.3",
        "typescript": "4.3.5"
    },
    "peerDependencies": {
        "@nestjs/common": "8.x",
        "@nestjs/core": "8.x",
        "@nestjs/terminus": "^8.0.0-next.0",
        "ioredis": "4.x"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "lib",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node",
        "verbose": true,
        "coveragePathIgnorePatterns": [
            "/node_modules/",
            "/interfaces/"
        ],
        "coverageReporters": [
            "json-summary"
        ]
    },
    "engines": {
        "node": "14.x",
        "npm": "7.x"
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "publishConfig": {
        "access": "public"
    },
    "homepage": "https://github.com/liaoliaots/nestjs-redis#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/liaoliaots/nestjs-redis.git"
    },
    "bugs": {
        "url": "https://github.com/liaoliaots/nestjs-redis/issues"
    }
}
