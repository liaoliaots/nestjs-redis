{
    "name": "@liaoliao/nestjs-redis",
    "version": "1.0.0",
    "description": "Redis(ioredis) module for NestJS framework",
    "author": "LiaoLiao <yxiaosong002@gmail.com> (https://github.com/liaoliaots)",
    "private": false,
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "license": "MIT",
    "keywords": [
        "redis",
        "cluster",
        "ioredis",
        "health",
        "nestjs"
    ],
    "scripts": {
        "prepare": "husky install",
        "prebuild": "rimraf dist",
        "build": "tsc -p tsconfig.build.json",
        "test": "jest",
        "test:cov": "jest --coverage",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "test:clear": "jest --clearCache",
        "madge": "madge --image dist/dependencies-graph.svg dist/index.js",
        "semantic-release": "semantic-release"
    },
    "dependencies": {
        "tslib": "^2.3.0"
    },
    "devDependencies": {
        "@commitlint/cli": "12.1.4",
        "@commitlint/config-angular": "12.1.4",
        "@nestjs/common": "7.6.17",
        "@nestjs/core": "7.6.17",
        "@nestjs/platform-fastify": "7.6.17",
        "@nestjs/terminus": "7.2.0",
        "@nestjs/testing": "7.6.17",
        "@types/ioredis": "4.26.4",
        "@types/jest": "26.0.23",
        "@types/node": "15.12.2",
        "@typescript-eslint/eslint-plugin": "4.26.1",
        "@typescript-eslint/parser": "4.26.1",
        "eslint": "7.28.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-eslint-comments": "3.2.0",
        "eslint-plugin-import": "2.23.4",
        "eslint-plugin-jest": "24.3.6",
        "eslint-plugin-prettier": "3.4.0",
        "eslint-plugin-tsdoc": "0.2.14",
        "husky": "6.0.0",
        "ioredis": "4.27.6",
        "jest": "27.0.4",
        "lint-staged": "11.0.0",
        "prettier": "2.3.1",
        "reflect-metadata": "0.1.13",
        "rimraf": "3.0.2",
        "semantic-release": "17.4.3",
        "ts-jest": "27.0.3",
        "typescript": "4.3.2"
    },
    "peerDependencies": {
        "@nestjs/common": "7.x",
        "@nestjs/core": "7.x",
        "@nestjs/terminus": "7.x",
        "ioredis": "4.x"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "lib",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node",
        "verbose": true,
        "coveragePathIgnorePatterns": [
            "/node_modules/",
            "/interfaces/"
        ],
        "setupFiles": [
            "../jest-env.ts"
        ]
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-angular"
        ]
    },
    "release": {
        "branches": ["main"]
    },
    "homepage": "https://github.com/liaoliaots/nestjs-redis#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/liaoliaots/nestjs-redis.git"
    },
    "bugs": {
        "url": "https://github.com/liaoliaots/nestjs-redis/issues"
    }
}
